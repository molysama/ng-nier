{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "downlevelIteration": true,
    "experimentalDecorators": true,
    "module": "esnext",
    "moduleResolution": "node",
    "importHelpers": true,
    "target": "es2015",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2018",
      "dom"
    ],
    "paths": {
      "button": [
        "dist/button"
      ],
      "button/*": [
        "dist/button/*"
      ],
      "fieldset": [
        "dist/fieldset"
      ],
      "fieldset/*": [
        "dist/fieldset/*"
      ],
      "checkbox": [
        "dist/checkbox"
      ],
      "checkbox/*": [
        "dist/checkbox/*"
      ],
      "select": [
        "dist/select"
      ],
      "select/*": [
        "dist/select/*"
      ],
      "content": [
        "dist/content"
      ],
      "content/*": [
        "dist/content/*"
      ],
      "title": [
        "dist/title"
      ],
      "title/*": [
        "dist/title/*"
      ]
    }
  },
  "angularCompilerOptions": {
    "enableIvy": false,
    "fullTemplateTypeCheck": true,
    "strictInjectionParameters": true
  }
}